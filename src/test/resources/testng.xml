<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="POST Test Suite">
    <parameter name="properties" value="src/test/resources/get.properties"/>
    <test name="POST request" preserve-order="true">
        <groups>
            <run>
                <include name="post"/>
            </run>
        </groups>
        <classes>
            <class name="org.sayem.api.json.placeholder.myGetTest.java">
                <methods>
                    <include name="postRequest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>